{
  "name": "viscreation/vis-ui-js",
  "type": "component",
  "description": "JavaScript UI generator framework based on jQuery UI  and some other elements",
  "license": "MIT",
  "authors": [
    {
      "name": "Andriy Oblivantsev",
      "email": "eslider@gmail.com"
    }
  ],
  "require": {
    "robloach/component-installer": "*",
    "components/normalize.css": "2.*",
    "components/font-awesome": "*",
    "components/bootstrap": "3.*",
    "drmonty/leaflet": "0.*",
    "components/openlayers2": "*",
    "components/jquery": "1.*",
    "components/jqueryui": "1.*",
    "afarkas/html5shiv": "dev-master",
    "components/underscore": "*",
    "components/respondjs": "*",
    "components/jquery-dialogextend-js": "*",
    "components/datatables": "1.10.*",
    "components/placeholdersjs": "*"
  },
  "config": {
    "component-dir": "web/assets"
  },
  "extra": {
    "component": {
      "scripts": [
        "js/utils/*.js",
        "js/elements/*.js",
        "js/jquery.form.generator.js"
      ],
      "styles" : [
        "css/base.css"
      ],
      "components/bootstrap": {
        "styles": [
          "css/bootstrap.min.css",
          "css/bootstrap-theme.min.css"
        ]
      }
    }
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "components/openlayers2",
        "type": "component",
        "version": "2.13.1",
        "dist": {
          "url": "https://github.com/openlayers/openlayers/releases/download/release-2.13.1/OpenLayers-2.13.1.zip",
          "type": "zip"
        },
        "extra": {
          "component": {
            "scripts": [
              "OpenLayers.js"
            ],
            "styles": [
              "theme/default/style.css"
            ],
            "files": [
              "*.js",
              "img/*",
              "art/*",
              "theme/*/*"
            ]
          }
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/respondjs",
        "type": "component",
        "version": "1.4.2",
        "source": {
          "url": "https://github.com/scottjehl/Respond",
          "type": "git",
          "reference": "master"
        },
        "extra": {
          "component": {
            "scripts": ["dest/respond.min.js"],
            "files": ["dest/*.js"]
          }
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/placeholdersjs",
        "type": "component",
        "version": "3.0.2",
        "source": {
          "url": "http://github.com/jamesallardice/Placeholders.js",
          "type": "git",
          "reference": "master"
        },
        "extra": {
          "component": {
            "scripts": ["dist/*.js"]
          }
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "components/jquery-dialogextend-js",
        "type": "component",
        "version": "2.0.4",
        "source": {
          "url": "http://github.com/ROMB/jquery-dialogextend",
          "type": "git",
          "reference": "master"
        },
        "extra": {
          "component": {
            "scripts": ["build/*.js"]
          }
        }
      }
    }
  ]
}